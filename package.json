{
  "name": "react-d3-travel-blog",
  "version": "0.0.1",
  "description": "A very simple travel blog, made of react and d3.",
  "repository": {
    "type": "git",
    "url": "https://github.com/alxkhr/react-d3-travel-blog.git"
  },
  "scripts": {
    "test": "jest",
    "start": "serve ./dist",
    "release": "rm -rf dist/ && cross-env NODE_ENV=production webpack -p --progress",
    "build": "rm -rf dist/ && webpack --progress"
