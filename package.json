    }
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "d3-geo": "^1.6.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router-dom": "^4.0.0",
    "topojson-client": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.1",
    "enzyme": "^2.7.1",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "markdown-loader": "^2.0.0",
    "marked": "^0.3.6",
    "node-sass": "^4.5.0",
